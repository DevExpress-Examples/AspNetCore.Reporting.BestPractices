  <ItemGroup>
    <ProjectReference Include="..\AspNetCore.Reporting.Common\AspNetCore.Reporting.Common.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Reports\CourseListReport.cs">
